{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-eslint-rules"
    ],
    "jsRules": {},
    "rules": {
        "ter-indent": [true, 4],
        "linebreak-style": [true, "LF"],
        "quotemark": [true, "single"],
        "semicolon": [true, "always"],
        "no-console": [false],
        "no-bitwise": [false]
    },
    "rulesDirectory": []
}
